<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>
        Nexship Inventory Management System
        @*@ViewBag.Title*@
    </title>
    <meta content=' initial-scale=1.0' name='viewport' />
    <!--Fonts and icons this include Roboto-->
    @*<link href="~/controls/material.css" rel="stylesheet" />*@
    <!--Metronic Theme Styles-->
    <link href="~ /metheme/dist/assets/media/logos/favicon.ico" />

    <link href="~/metheme/dist/assets/plugins/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter:300,400,500,600,700" />
    <link href="~/metheme/dist/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Vendor Stylesheets-->
    <!--begin::Global Stylesheets Bundle(mandatory for all pages)-->
    <link href="~/metheme/dist/assets/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/metheme/dist/assets/css/style.bundle.css" rel="stylesheet" type="text/css" />

    @*<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">*@
    <!--end::Global Stylesheets Bundle-->
    <!--Bootstrap-->
    @* <link href="~/assets/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/assets/css/material-dashboard.css" rel="stylesheet" />*@

    <!-- CSS Files -->
    @*<link href="~/assets/mfb/mfb.css" rel="stylesheet" />
    <link href="~/assets/lib/bootstrap/css/custom-css.css" rel="stylesheet" />*@

    <!--  Plugin for Alert -->
    @* <link href="~/assets/lib/toaster/toastr.min.css" rel="stylesheet" />*@

    <script src="~/assets/js/core/jquery.min.js"></script>
    <!--CoreJs Files-->
    <script src="~/corejs/core.js"></script>
    <!--Metronic Js Files-->
    <script src="~/metheme/dist/assets/plugins/global/plugins.bundle.js"></script>

    <script src="~/metheme/dist/assets/js/scripts.bundle.js"></script>
    

    <!--begin::Custom Javascript(used for this page only)-->
    <script src="~/metheme/dist/assets/js/widgets.bundle.js"></script>
    <script src="~/metheme/src/js/custom/widgets.js"></script>
    <script src="~/metheme/dist/assets/js/custom/apps/chat/chat.js"></script>
    <script src="~/metheme/dist/assets/js/custom/utilities/modals/upgrade-plan.js"></script>
    <script src="~/metheme/dist/assets/js/custom/utilities/modals/create-app.js"></script>
    <script src="~/metheme/dist/assets/js/custom/utilities/modals/new-target.js"></script>
    <script src="~/metheme/dist/assets/js/custom/utilities/modals/users-search.js"></script>
    <script src="~/metheme/dist/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js"></script>
    <script src="~/metheme/dist/assets/plugins/custom/datatables/datatables.bundle.js"></script>

    @*<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.min.js" integrity="sha384-Rx+T1VzGupg4BHQYs2gCW9It+akI2MM/mndMCy36UVfodzcJcF0GGLxZIzObiEfa" crossorigin="anonymous"></script>*@
    <!--end::Custom Javascript-->
    <!--  Plugin for Alert -->
   @* <script src="~/assets/lib/toaster/toastr.min.js"></script>*@
    @*@RenderSection("Header", required: false)*@
</head>
<body id="kt_app_body" data-kt-app-layout="dark-sidebar" data-kt-app-header-fixed="true" data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true" data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true" data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default">
    <!--begin::App-->
    <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
        <!--begin::Page-->
        <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
            <!--begin::Header-->
            <div id="kt_app_header" class="app-header" data-kt-sticky="true" data-kt-sticky-activate="{default: true, lg: true}" data-kt-sticky-name="app-header-minimize" data-kt-sticky-offset="{default: '200px', lg: '0'}" data-kt-sticky-animation="false">
                @await Component.InvokeAsync("Header")
            </div>
            <!--Begin:Wrapper-->
            <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
                <!--Begin:Sidebar-->
                @await Component.InvokeAsync("Sidebar")
                <!--End:Sidebar-->
                <!--Begin:Main-->
                <div class="app-main flex-column flex-row-fluid" id="kt_app_main">
                    <!--Begin:ContentWrapper-->
                    @RenderBody()
                    <!--End:ContentWrapper-->
                </div>
                <!--End:Main-->
                <!--Begin:Footer-->
                <div id="kt_app_footer" class="app-footer">
                    @await Component.InvokeAsync("Footer")
                </div>
                <!--End:Footer-->
            </div>
            <!--End:Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::App-->
    <!-- for Data -->
    @*<script src="~/assets/js/core/bootstrap-material-design.min.js" type="text/javascript"></script>
    <script src="~/assets/js/material-dashboard.js?v=2.1.2" type="text/javascript"></script>
    <script src="~/assets/js/plugins/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>*@

    <!-- Plugin for the momentJs  -->
   @* <script src="~/assets/js/plugins/moment.min.js"></script>*@
    <!-- Section for Pages Scripts To Run-->
    @RenderSection("Scripts", required: false)
</body>
</html>
